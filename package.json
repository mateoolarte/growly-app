{
  "name": "growly",
  "version": "1.0.0",
  "private": true,
  "eslintConfig": {
    "extends": [
      "next",
      "prettier"
    ]
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "fmt": "prettier --write .",
    "pretty": "yarn fmt && yarn lint",
    "test:jest": "jest",
